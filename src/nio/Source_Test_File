function getEasyExam(id) {
    var items = document.getElementsByName('exam_type');
    for (var i=0; i < items.length; i++) {
        items[i].disabled = true;
    }
    if(id) {
        //alert("---"+id);
        for (var i=0; i < items.length; i++) {
            items[i].className = 'btn-css marks-btn';
        }
        document.getElementById(id).className = 'btn-css active-btn';
    }else{
        id = 'Easy';
    }
//alert('ajax called');
    $.ajax({
        url: "http://localhost:8080/examdetail/"+id+"/1Z0-813/JAVA",
        category : 'EASY',
        type : "GET",
        exam : '',
        dataType: "json",
        success: function(data) {
            if(!data || data.length == 0){
                for (var i=0; i < items.length; i++) {
                    items[i].disabled = false;
                }
            }
            var json1 = JSON.parse(JSON.stringify(data));
            console.log(json1[0].option3);
            var ans="";
            var count=0;
            $("#examText").empty();
            for (var i=0;i<json1.length;++i)
            {
                count++;
                var obj = json1[i];
                var options="";
                var multiple_ans=0;
                if(!obj.answerDescription || obj.answerDescription == ''){
                    obj.answerDescription = 'Explaining is being added, please visit again to see it.';
                }
                if(obj.has_multiple_ans == "0"){
                    if(obj.option1 && obj.option1 != ''){
                        options =options+'<label class="hideOptions" id="optionLabel'+count+'1"><input class="radio-btn" type="radio" name="answer'+count+'" id="id'+count+'1" value="option'+count+'1" />'+obj.option1+'</label><br>';
                    }
                    if(obj.option2 && obj.option2 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'2"><input class="radio-btn" type="radio" name="answer'+count+'" id="id'+count+'2" value="option'+count+'2" />'+obj.option2+'</label><br>';
                    }
                    if(obj.option3 && obj.option3 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'3"><input class="radio-btn" type="radio" name="answer'+count+'" id="id'+count+'3" value="option'+count+'3" />'+obj.option3+'</label><br>';
                    }
                    if(obj.option4 && obj.option4 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'4"><input class="radio-btn" type="radio" name="answer'+count+'" id="id'+count+'4" value="option'+count+'4" />'+obj.option4+'</label><br>';
                    }
                    if(obj.option5 && obj.option5 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'5"><input class="radio-btn" type="radio" name="answer'+count+'" id="id'+count+'5" value="option'+count+'5" />'+obj.option5+'</label><br>';
                    }
                    if(obj.option6 && obj.option6 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'6"><input class="radio-btn" type="radio" name="answer'+count+'" id="id'+count+'6" value="option'+count+'6" />'+obj.option6+'</label><br>';
                    }
                    if(obj.option7 && obj.option7 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'7"><input class="radio-btn" type="radio" name="answer'+count+'" id="id'+count+'7" value="option'+count+'7" />'+obj.option7+'</label><br>';
                    }
                    $("#examText").append('<div class="read">'
                        +(i+1)+':'+obj.question+'</br></br>'+
                        '<div class=option>'+options+ '</div>'+
                        '<label class="ans" name = "'+obj.answer+'" ans="'+ans+'" style="display:none;margin-top:3%;" id="answer'+count+'" >'+
                        'Answer : Option '+obj.answer+'</label><span id="span'+count+'"></span>'+
                        '<br><button class="hide" id="ansDesc'+count+'" onclick="AnsDesc(&#39;AD'+count+'&#39;)">Answer Explanation</button>'+
                        '<div class="ansD_div" id="AD'+count+'" style="display:none">'+obj.answerDescription+'</div>'+
                        '<br>'+
                        '<button class="exam_button" id="show'+count+'" onclick="showAns(&#39;answer'+count+'&#39;)" style="margin-right:3%;">Submit Answer</button>'+
                        //'<button style="display:" id="hide'+count+'" class="exam_button" onclick="hideAns(&#39;answer'+count+'&#39;)">Hide Answer</button>'+
                        '</br>'+
                        '</div></br>');
                }else{
                    if(obj.option1 && obj.option1 != ''){
                        options =options+'<label class="hideOptions" id="optionLabel'+count+'1"><input class="radio-btn" type="checkbox" name="answer'+count+'" id="id'+count+'1" value="option'+count+'1" />'+obj.option1+'</label><br>';
                    }
                    if(obj.option2 && obj.option2 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'2"><input class="radio-btn" type="checkbox" name="answer'+count+'" id="id'+count+'2" value="option'+count+'2" />'+obj.option2+'</label><br>';
                    }
                    if(obj.option3 && obj.option3 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'3"><input class="radio-btn" type="checkbox" name="answer'+count+'" id="id'+count+'3" value="option'+count+'3" />'+obj.option3+'</label><br>';
                    }
                    if(obj.option4 && obj.option4 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'4"><input class="radio-btn" type="checkbox" name="answer'+count+'" id="id'+count+'4" value="option'+count+'4" />'+obj.option4+'</label><br>';
                    }
                    if(obj.option5 && obj.option5 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'5"><input class="radio-btn" type="checkbox" name="answer'+count+'" id="id'+count+'5" value="option'+count+'5" />'+obj.option5+'</label><br>';
                    }
                    if(obj.option6 && obj.option6 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'6"><input class="radio-btn" type="checkbox" name="answer'+count+'" id="id'+count+'6" value="option'+count+'6" />'+obj.option6+'</label><br>';
                    }
                    if(obj.option7 && obj.option7 != ''){
                        options = options+'<label class="hideOptions" id="optionLabel'+count+'7"><input class="radio-btn" type="checkbox" name="answer'+count+'" id="id'+count+'7" value="option'+count+'7" />'+obj.option7+'</label><br>';
                    }
                    $("#examText").append('<div class="read">'
                        +(i+1)+':'+obj.question+'</br></br>'+
                        '<div class=option>'+options+ '</div>'+
                        '<label class="ans" name = "'+obj.answer+'" ans="'+ans+'" style="display:none;margin-top:3%;" id="answer'+count+'" >'+
                        'Answer : Option '+obj.answer+'</label><span id="span'+count+'"></span>'+
                        '<br><button class="hide" id="ansDesc'+count+'" onclick="AnsDesc(&#39;AD'+count+'&#39;)">Answer Explanation</button>'+
                        '<div class="ansD_div" id="AD'+count+'" style="display:none">'+obj.answerDescription+'</div>'+
                        '<br>'+
                        '<button class="exam_button" id="show'+count+'" onclick="showAns(&#39;answer'+count+'&#39;)" style="margin-right:3%;">Submit Answer</button>'+
                        //'<button style="display:" id="hide'+count+'" class="exam_button" onclick="hideAns(&#39;answer'+count+'&#39;)">Hide Answer</button>'+
                        '</br>'+
                        '</div></br>');
                }
            }
            console.log(data);
            document.getElementById('totalQues').innerText = json1.length;
            for (var i=0; i < items.length; i++) {
                items[i].disabled = false;
            }
        },failure: function(data) {
            for (var i=0; i < items.length; i++) {
                items[i].disabled = false;
            }
        }
    });
}

function showAns(id) {
    //alert(e.getAttribute('name'));
    if(!document.querySelector('input[name="'+id+'"]:checked')){
        alert("Choose an option to see answer");
        return;
    }
    var e = document.getElementById(id);
    var quesNo = id.substring("answer".length,id.length);
    var answer = e.getAttribute('name');
    if(answer.indexOf(',') > 0){
        var ansArray = answer.split(',');
        // var selectedOptId = document.querySelector('input[name="' + id + '"]:checked').id;
        var selectedIDs = [];
        $(':checkbox[name="' + id + '"]:checked').each (function () {
            selectedIDs.push(this.id.substring(this.id.length-1));
        });
        console.log(selectedIDs);
        if(JSON.stringify(ansArray) == JSON.stringify(selectedIDs)){
            /*$(':checkbox[name="' + id + '"]:checked').each (function () {
                selectedIDs = this.id.substring(selectedIDs.length-1) + ',';
                document.getElementById(this.id).style.background = "burlywood";
            });*/
            selectedIDs.forEach(ans => {console.log(ans)
                document.getElementById('optionLabel'+quesNo+ans).style.background = "burlywood";});
            document.getElementById('span'+quesNo).innerHTML = "&#10003;";
            document.getElementById('span'+quesNo).className = 'tick';
            document.getElementById('scoreLabel').innerText = Number(document.getElementById('scoreLabel').innerText) +  1;
        }else{
            ansArray.forEach(ans => {console.log(ans)
                document.getElementById('optionLabel'+quesNo+ans).style.background = "burlywood";});
            document.getElementById('span'+quesNo).innerHTML = "&#9747;";
            document.getElementById('span'+quesNo).className = 'cross';
        }
    } else {
        var selectedOptId = document.querySelector('input[name="' + id + '"]:checked').id;
        if(answer == selectedOptId.substring(selectedOptId.length-1)){
            document.getElementById('optionLabel'+quesNo+e.getAttribute('name')).style.background = "burlywood";
            document.getElementById('span'+quesNo).innerHTML = "&#10003;";
            document.getElementById('span'+quesNo).className = 'tick';
            document.getElementById('scoreLabel').innerText = Number(document.getElementById('scoreLabel').innerText) +  1;
        }else{
            document.getElementById('optionLabel'+quesNo+e.getAttribute('name')).style.background = "burlywood";
            document.getElementById('span'+quesNo).innerHTML = "&#9747;";
            document.getElementById('span'+quesNo).className = 'cross';
        }
    }
    e.style.display = 'block';
    document.getElementById('show'+quesNo).style.display = 'none';
    document.getElementById('ansDesc'+quesNo).className = 'exam_button';

    //document.getElementById('hide'+quesNo).style.display = 'block';
    /*if(e.style.display == 'none')
           e.style.display = 'block';
     else
           e.style.display = 'none';
     if(id){

     }*/
}
function hideAns(id) {
    var e = document.getElementById(id);
    var quesNo = id.substring(id.length-1);
    if(e.style.display == 'none')
        e.style.display = 'none';
    else
        e.style.display = 'block';
    document.getElementById('hide'+quesNo).style.display = 'none';
    document.getElementById('show'+quesNo).style.display = 'block';
}
function AnsDesc(id){
    var e = document.getElementById(id);
    if(e.style.display == 'none')
        e.style.display = 'block';
    else
        e.style.display = 'none';
}

