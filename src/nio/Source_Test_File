var varFileName_Description;
var packageNames;
//get description of selected file
function getFilePackage() {
    $("#description").empty();
    package = document.getElementById("package");
    if(!packageNames || packageNames.length == 0) {
        $.get("http://127.0.0.1:8083/filePackage",
            function (data, status) {
                //alert("Data: " + data + "\nStatus: " + status);
                // var fileNames = document.getElementById("file");
                var packageNames = document.getElementById("package");
                var option;
                for (var j = 0; j < data.length; j++) {
                    //console.log(data[j].web_name);
                    //console.log(data[j].description);
                    //file_option = document.createElement("option");
                    // file_option.text = data[j].web_name;
                    //  fileNames.add(file_option);
                    package_option = document.createElement("option");
                    package_option.text = data[j].Tables_in_navneetrai;
                    package.add(package_option);
                    packageNames = data;
                }
                getFileMetadata();
            });
    }else{
        for (var j = 0; j < packageNames.length; j++) {
            package_option = document.createElement("option");
            package_option.text = data[j].category;
            packageNames.add(package_option);
        }
        getFileMetadata();
    }
}
function getFileMetadata() {
    $("#description").empty();
    $("#file").empty();
    package = document.getElementById("package").value;
    fileNames = document.getElementById("file");
    // if(!varFileName_Description || varFileName_Description.length == 0) {
    $.post("http://127.0.0.1:8083/fileMetadata",
        {
            web_url:'http://javacommunity.org/java/'+window.location.href.substring(window.location.href.lastIndexOf('/')+1,window.location.href.length),
            package : package
        },
        function(data, status){
            var option ;
            for (var j = 0; j < data.length; j++){
                //  console.log('--webma --'+data[j].web_name);
                option = document.createElement("option");
                option.text = data[j].web_name;
                fileNames.add(option);
                varFileName_Description = data;
            }
            getDescForThisFile();
        });
    // }
}
