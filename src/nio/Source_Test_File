function SubmitUrl() {

    var data = [];
    var searchEles = $('#append-div').children().children() ;
    console.log(searchEles);
    for(var i = 0; i < searchEles.length; i++) {
        if(searchEles[i].tagName == 'INPUT') {
            if(searchEles[i].id.indexOf('UrlText') == 0) {
                data.push(searchEles[i].value);

            }
        }
    }
    console.log(data);
    $.ajax({
        type: 'POST',
        data: {data:data},
        // contentType: 'application/json',
        url: 'http://117.220.9.53:8082/urlAllSubmit',
        success: function(data) {
            console.log('success');
            console.log('check data',data);
            var key;
            var i=0;
            for (key in data) {
                i++;
                if (data.hasOwnProperty(key)) {
                    console.log(key + " = " + data[key]);
                    var status = data[key];
                    document.getElementById('GetStatusInput'+i).value = JSON.stringify(status);
                }
            }
        },
        error: function(data){
            $('#'+statusId).val(data.status);
            console.log('error');
            console.log(data.status);
            // alert("enter valid url");
            // alert( " returns a " + data.status);
            //console.log('error');
        }

    });



//            var alldescendants = [];

