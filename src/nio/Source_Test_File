function returnScore(){
    var tempNames = [];
    var answers = ["option21","option42","option63","option44","option35","option46","option17","option38","option49","option410","option511","option112","option413","option314","option415","option416","option317","option318","option219","option320","option221"],
        total = answers.length;
    var radios = document.querySelectorAll('input').forEach(function(item) {

        if (tempNames.indexOf(item.name) === -1) {
            tempNames.push(item.name)
        }
    })
    for (var i = 0; i < tempNames.length; i++) {
        if (document.querySelector('input[name="' + tempNames[i] + '"]:checked') !== null) {
            console.log(document.querySelector('input[name="' + tempNames[i] + '"]:checked').value)
        } else {
            console.log("Radio button of group value " + tempNames[i] + " is unchecked")

        }

    }
}

function recScore(ans){
    var ansStr = ""+ans;
    if(ansStr.indexOf(',') == -1){
        //$("#"+inputs.item(i).id).prop("checked", true);
        //alert(ans);
        alert($(this).closest('label').text());
    }else{
        //$("#"+inputs.item(i).id).prop("checked", true);
        //alert(ans);
        alert($(this).closest('label').text());
    }
}
