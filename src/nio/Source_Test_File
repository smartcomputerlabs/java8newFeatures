    while(true){
        $("#"+id1).css("display", "none");
    }
}
function hideAllAns() {
    while(true){
        $("#"+id1).css("display", "none");
    }
}
// create File
function createFile(){
    var fileName = document.getElementById("new_file").value;
    var packageName = document.getElementById("new_package").value;
    if(! fileName || ! packageName){
        alert('Please add Package and File name');
        return;
    }

    else {
        $.post("http://127.0.0.1:8080/createFile",{
                web_url:'http://javacommunity.org/'+document.getElementById("new_package").value+'/'+document.getElementById("new_file")+'.html',
                fileName : fileName,
                packageName : packageName,
                heading : document.getElementById("new_heading").value,
                descHeading : document.getElementById("new_desc_heading").value,
                desc : document.getElementById("new_desc").value
            },
            function (data, status) {
                alert("Data: " + data + "\nStatus: " + status);

            });
        //getFileMetadata();
    }
}
