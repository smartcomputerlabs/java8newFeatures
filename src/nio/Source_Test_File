//---------------------------------------- comments----------------------------------
var form = document.querySelector('.form-comment');
var intbtn = 0;
function validateForm(){
    var comment = $('.commentar').val();
    var name = $('.commentTextBox').val();
    var email =$('.commentEmail').val();
    var button = document.querySelector('.submit');
    var regEx = /\S+@\S+\.\S+/;
    var letters = /[A-Za-z]/;

    if(!comment && !name && !email){
        document.getElementById("error_textarea").innerHTML = "**Add Comment";
        document.getElementById("textarea").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_textarea").innerHTML = '';},3000);
        document.getElementById("error_name").innerHTML = "**Enter Name (alphabets only)";
        document.getElementById("inputForName").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_name").innerHTML = '';},3000);
        document.getElementById("error_email").innerHTML = "**Enter Correct Email";
        document.getElementById("inputForEmail").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_email").innerHTML = '';},3000);
        return false;}
    else if(!comment && !name){
        document.getElementById("error_textarea").innerHTML = "**Add Comment";
        document.getElementById("textarea").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_textarea").innerHTML = '';},3000);
        document.getElementById("error_name").innerHTML = "**Enter Name (alphabets only)";
        document.getElementById("inputForName").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_name").innerHTML = '';},3000);
        return false;}
    else if(!comment && !email){
        document.getElementById("error_textarea").innerHTML = "**Add Comment";
        document.getElementById("textarea").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_textarea").innerHTML = '';},3000);
        document.getElementById("error_email").innerHTML = "**Enter Correct Email";
        document.getElementById("inputForEmail").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_email").innerHTML = '';},3000);
        return false;}
    else if(!name && !email){
        document.getElementById("error_name").innerHTML = "**Enter Name (alphabets only)";
        document.getElementById("inputForName").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_name").innerHTML = '';},3000);
        document.getElementById("error_email").innerHTML = "**Enter Correct Email";
        document.getElementById("inputForEmail").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_email").innerHTML = '';},3000);
        return false;}
    else if(!comment){
        document.getElementById("error_textarea").innerHTML = "**Add Comment";
        document.getElementById("textarea").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_textarea").innerHTML = '';},3000);
        return false;
    }
    else if(!name){
        document.getElementById("error_name").innerHTML = "**Enter Name (alphabets only)";
        document.getElementById("inputForName").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_name").innerHTML = '';},3000);
        return false;
    }
    else if(!regEx.test(email)){
        document.getElementById("error_email").innerHTML = "**Enter Correct Email";
        document.getElementById("inputForEmail").style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_email").innerHTML = '';},3000);
        return false;
    }
    else {
        intbtn++;
        el = document.createElement('span');
        el.className = "box_result row-comment";
        el.innerHTML =
            '<div class="avatar_comment col-md-1">'+
            '<img src="../img/user.png" alt="avatar"/>'+
            '</div>'+
            '<div class="result_comment col-md-11">'+
            '<h4>'+ name +'</h4>'+
            '<label class="emailLabel">'+ email +'</label>'+
            '<p>'+ comment +'</p>'+
            '<div class="tools_comment">'+
            '<a class="like" id="like'+intbtn+'"><i class="fa fa-thumbs-up"></i><input type="text" class="thumbUp-in"  name="txt" value="0" readonly></a>'+
            '<a class="dislike" id="dislike'+intbtn+'"><i class="fa fa-thumbs-down" aria-hidden="true"></i><input type="text" class="thumbDown-in" name="txt" value="0" readonly></a>'+
            '<a class="replay" href="#"><i class="fa fa-reply" aria-hidden="true"></i>Reply</a>'+
            '</div>'+

            '</div>'+
            '<div class="child_replay"></div>';
        document.getElementById('list_comment').prepend(el);
        $('.commentar').val('');
        $('.commentTextBox').val('');
        $('.commentEmail').val('');
        console.log(comment);
        $(function(){
            $("#like"+intbtn).click(function(){
                var input = $(this).find('.thumbUp-in');
                input.val(parseInt(input.val())+1);
            });
            $("#dislike"+intbtn).click(function(){
                var input = $(this).find('.thumbDown-in');
                input.val(parseInt(input.val())+1);
            });

        })
        return false;
    }
}
form.addEventListener('submit', function(event){ event.preventDefault();})

// --------------------------Reply Comment------------------------------------
var replyid =0;
$(document).ready(function() {
    replyid++;
    $('#list_comment').on('click', '.replay', function (e) {
        cancel_reply();
        $current = $(this);
        el = document.createElement('span');
        el.className = "box_reply row-comment";
        el.innerHTML =
            '<div class="col-md-12 reply_comment" style="width:100%;">'+
            '<div class="row-comment " id="target'+replyid+'" >'+
            '<div class="avatar_comment col-md-1" style="margin-top: 0%; padding:none;">'+
            '<img src="../img/user.png" alt="avatar"/>'+
            '</div>'+
            '<div class="box_comment  z" tabindex="0" id="z'+replyid+'">'+
            '<form class="formsR">'+
            '<textarea class="comment_replay red_text" id="textareaR'+replyid+'" placeholder="Reply a comment..." ></textarea>'+
            '<div class="box_post">'+
            '<p><span id="error_textareaReply"></span></p>'+
            '<div class="pull-right">'+
            '<button class="cancel" onclick="cancel_reply()" type="button">Cancel</button>'+
            '<button onclick="submit_reply()" type="button" class="submitR "  value="1" >Reply</button>'+
            '</div>'+
            '<input type="text" name="text" class="commentTextBoxReply red_name" id="CommentnameR'+replyid+'" placeholder="Enter Your Name Here...**" '+
            ' onkeypress="return (event.charCode > 64 &&  event.charCode < 91) || (event.charCode > 96 && event.charCode < 123)" required><br>'+
            '<p><span id="error_nameReply"></span></p>'+
            '<input type="email" name="emailR" class="commentEmailReply red_email"  placeholder="Enter Your Email...**" id="emailReply'+replyid+'">'+
            '<p><span id="error_emailReply"></span></p>'+
            '</div>'+
            '</form>'+
            '</div>'+
            '</div>'+
            '</div>';
        $current.closest('span').find('.child_replay').prepend(el);
        //$('.z')[0].focus();
        setTimeout(function(){
            $("#z"+replyid).focus();},100);
        //document.getElementById("target").focus({preventScroll:false});
        //target.querySelector(".z").focus();
    });
});


var likeDlike =0;
//var replyid1 =0;
function submit_reply(){
    //replyid1++;
    likeDlike++;
    var comment_replay = $('.comment_replay').val();
    var nameReply = $('.commentTextBoxReply').val();
    var emailReply =$('.commentEmailReply').val();
    var button = document.querySelector('.submitR');
    var regEx = /\S+@\S+\.\S+/;
    var letters = /^[A-Za-z]+$/;
    if(!comment_replay && !nameReply && !emailReply){
        document.getElementById("error_textareaReply").innerHTML = "**Add Comment";
        //document.getElementById("textareaR"+likeDlike).style.border=" 1px solid red ";
        document.getElementsByClassName('red_text')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_textareaReply").innerHTML = '';},3000);
        document.getElementById("error_nameReply").innerHTML = "**Enter Name (alphabets only)";
        document.getElementsByClassName('red_name')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_nameReply").innerHTML = '';},3000);
        document.getElementById("error_emailReply").innerHTML = "**Enter Correct Email";
        document.getElementsByClassName('red_email')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_emailReply").innerHTML = '';},3000);
        // return false;
    }
    else if( !comment_replay && !nameReply ){
        document.getElementById("error_textareaReply").innerHTML = "**Add Comment";
        //document.getElementById("textareaR"+likeDlike).style.border=" 1px solid red ";
        document.getElementsByClassName('red_text')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_textareaReply").innerHTML = '';},3000);
        document.getElementById("error_nameReply").innerHTML = "**Enter Name (alphabets only)";
        document.getElementsByClassName('red_name')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_nameReply").innerHTML = '';},3000);
    }
    else if (!comment_replay && !emailReply) {
        document.getElementById("error_textareaReply").innerHTML = "**Add Comment";
        document.getElementsByClassName('red_text')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_textareaReply").innerHTML = '';},3000);
        document.getElementById("error_emailReply").innerHTML = "**Enter Correct Email";
        document.getElementsByClassName('red_email')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_emailReply").innerHTML = '';},3000);
    }
    else if(!nameReply && !emailReply){
        document.getElementById("error_nameReply").innerHTML = "**Enter Name (alphabets only)";
        document.getElementsByClassName('red_name')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_nameReply").innerHTML = '';},3000);
        document.getElementById("error_emailReply").innerHTML = "**Enter Correct Email";
        document.getElementsByClassName('red_email')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_emailReply").innerHTML = '';},3000);
    }
    else if(!comment_replay){
        document.getElementById("error_textareaReply").innerHTML = "**Add Comment";
        document.getElementsByClassName('red_text')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_textareaReply").innerHTML = '';},3000);
    }

    else if(!nameReply){
        document.getElementById("error_nameReply").innerHTML = "**Enter Name (alphabets only)";
        document.getElementsByClassName('red_name')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_nameReply").innerHTML = '';},3000);
    }
    else if(!regEx.test(emailReply)){
        document.getElementById("error_emailReply").innerHTML = "**Enter Correct Email";
        document.getElementsByClassName('red_email')[0].style.border="1px solid red";
        setTimeout(function(){document.getElementById("error_emailReply").innerHTML = '';},3000);

    }
    else{
        likeDlike++;
        el = document.createElement('span');
        el.className = "box_reply row-comment";
        el.innerHTML =
            '<div class="avatar_comment col-md-1">'+
            '<img src="../img/user.png" alt="avatar"/>'+
            '</div>'+
            '<div class="result_comment col-md-11">'+
            '<h4>'+ nameReply+'</h4>'+
            '<label class="emailLabel">'+ emailReply +'</label>'+
            '<p>'+ comment_replay +'</p>'+
            '<div class="tools_comment">'+
            '<a class="like" id="likeR'+likeDlike+'"><i class="fa fa-thumbs-up"></i><input type="text" class="thumbUp-inReply" name="txt" value="0" readonly></a>'+
            '<a class="dislike" id="dislikeR'+likeDlike+'"><i class="fa fa-thumbs-down" aria-hidden="true"></i><input type="text" class="thumbDown-inReply"  name="txt" value="0" readonly></a>'+
            '<a class="replay" href="#"><i class="fa fa-reply" aria-hidden="true"></i>Reply</a>'+
            '</div>'+

            '</div>'+
            '<div class="child_replay"></div>';
        $current.closest('span').find('.child_replay').prepend(el);
        $('.comment_replay').val('');
        $('.commentTextBoxReply').val('');
        $('.commentEmailReply').val('');
        cancel_reply();

        $(function(){
            $("#likeR"+likeDlike).click(function(){
                var input = $(this).find('.thumbUp-inReply');
                input.val(parseInt(input.val())+1);
            });
            $("#dislikeR"+likeDlike).click(function(){
                var input = $(this).find('.thumbDown-inReply');
                input.val(parseInt(input.val())+1);
            });
        })
    }
}

// -----------cancel comment reply---------------
function cancel_reply(){
    $('.reply_comment').remove();
}

// -----------------auto expand textarea-----------------------------

var textarea = document.getElementById("textarea");
var heightLimit = 2000;
textarea.oninput = function() {
    textarea.style.height = "";
    textarea.style.height = Math.min(textarea.scrollHeight, heightLimit) + "px";
};


