// textarea omn ky press to allow usage of tab
/*document.querySelector('#textarea').addEventListener('keydown', e => {
    if ( e.key === 'Tab' && !e.shiftKey ) {
        // execCommand operations are "Cmd|Ctrl+Z"-able
        // note: execCommand is deprecated and may not work in the future
        document.execCommand('insertText', false, "\t");
        e.preventDefault();
        return false;
    }
});*/
function allowTabInTextarea(event) {
    var char = event.which || event.keyCode;
    console.log(char);
    //alert(char);
    if (char === 9) { // tab was pressed
        event.preventDefault();
        // get caret position/selection
        var val = event.target.value,
            start = event.target.selectionStart,
            end = event.target.selectionEnd;

        // set textarea value to: text before caret + tab + text after caret
        event.target.value = val.substring(0, start) + '\t' + val.substring(end);

        // put caret at right position again
        event.target.selectionStart = event.target.selectionEnd = start + 1;
        //document.getElementById("heading")
        // event.target.value = event.target.value + '\t';
        // prevent the focus lose
        return false;
    }else{
        return true;
    }
};
//get description of selected file
function getDescForThisFile(){
    $("#description").empty();
    var fileName = document.getElementById("file").value;
    for (var j = 0; j < varFileName_Description.length; j++){
        if(varFileName_Description[j].web_name === fileName){
            //  console.log('fileName = '+varFileName_Description[j].web_name+' and fileNAme = '+document.getElementById("file").value)
            document.getElementById("heading").value = varFileName_Description[j].heading;
            document.getElementById("desc_heading").value = varFileName_Description[j].desc_heading;
            document.getElementById("jsEditor").remove();
            var jsEditor = $("<div id='jsEditor' className='resizeable'</div>");
            $('#editor').append(jsEditor);
            getcode('http://javacommunity.org/java/'+fileName+'.html');
            var descLines = varFileName_Description[j].description.split( "_^" );
            var results = [];
            if(descLines.length>1){
                $("#description").empty();
                for( var i = 0; i < descLines.length; ++i ) {
                    //results.push( descLines[i] );
                    //    console.log(descLines[i]);
                    count = i+1;
                    var newDiv = $("<div id='desc"+count+"'><label id='rowNumber"+count+"'>"+count+"</label><textarea id='descText"+count+"' class='descText' onkeydown='allowTabInTextarea(event)'>"+descLines[i]+"</textarea><button id='deleteDesc"+count+"' onClick="+"'deleteDescDiv(this);'"+">Delete</button></div>");
                    $('#description').append(newDiv);
                }
            }else{
                $("#description").empty();
                count = 1;
                var newDiv = $("<div id='desc"+count+"'><label id='rowNumber"+count+"'>"+count+"</label><textarea id='descText"+count+"' class='descText' onkeydown='allowTabInTextarea(event)'>"+varFileName_Description[j].description+"</textarea><button id='deleteDesc"+count+"' onClick="+"'deleteDescDiv(this);'"+">Delete</button></div>");
                $('#description').append(newDiv);
            }
        }
    }
}
// update description of a file
