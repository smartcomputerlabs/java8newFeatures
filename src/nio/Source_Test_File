    $('#description').append(newDiv);
    //alert($('#description').children().size());
}
// delete description of a file
function deleteDescDiv(e) {
    //alert(e.id);
    removal_div_num = e.id.substring(e.id.indexOf('deleteDesc')+10,e.id.length);
    // alert('removal_div_num = '+removal_div_num);
    var divs = document.getElementById("description").childNodes.length;
    var desc = '';
    for(var i=1;i<divs;i++){
        console.log('text = '+document.getElementById("desc"+i).childNodes[1].value);
        if(i != removal_div_num) {
            if (i == divs-1) {
                desc = desc + document.getElementById("descText"+i).innerHTML;
            } else {
                desc = desc + document.getElementById("descText"+i).innerHTML + '_^';
            }
        }
    }
    var descLines = desc.split( "_^" );
    var results = [];
    if(descLines.length>1){
        $("#description").empty();
        for( var i = 0; i < descLines.length; ++i ) {
            //results.push( descLines[i] );
            console.log(descLines[i]);
            count = i+1;
            var newDiv = $("<div id='desc"+count+"'><label id='rowNumber"+count+"'>"+count+"</label><textarea id='descText"+count+"' class='descText' onkeydown='allowTabInTextarea(event)'>"+descLines[i]+"</textarea><button id='deleteDesc"+count+"' onClick="+"'deleteDescDiv(this);'"+">Delete</button></div>");
            $('#description').append(newDiv);
        }
    }else{
        $("#description").empty();
        count = 1;
        var newDiv = $("<div id='desc"+count+"'><label id='rowNumber"+count+"'>"+count+"</label><textarea id='descText"+count+"' class='descText' onkeydown='allowTabInTextarea(event)'>"+varFileName_Description[j].description+"</textarea><button id='deleteDesc"+count+"' onClick="+"'deleteDescDiv(this);'"+">Delete</button></div>");
        $('#description').append(newDiv);
    }
}
// capitalization
function capitalizeFirstLetter(string) {
    return string.charAt(0).toUpperCase() + string.slice(1);
}
// update description of a file
function createContent() {//alert(document.getElementById("editor").value);
    var divs = document.getElementById("description").childNodes.length;
    heading = capitalizeFirstLetter(document.getElementById("heading").value);
    desc_heading = capitalizeFirstLetter(document.getElementById("desc_heading").value);
    var desc = '';
    //alert(divs);
    for(var i=1;i<=divs;i++){
        console.log('text = '+document.getElementById("desc"+i).childNodes[1].value.trim());
        if(i!=divs){
            //desc = desc+document.getElementById("desc"+i).childNodes[1].value.trim()+'_^';
            desc =  desc + document.getElementById("descText"+i).value+'_^';
            //= t.replace(/\n\r?/g, '<br />')
            //alert(desc);
        }
        else{
            desc =  desc + document.getElementById("descText"+i).value;
            alert(document.getElementById("descText"+i).value);
        }
    }
    //alert(desc);
    //alert(document.getElementById("file").value);
    $.post("http://127.0.0.1:8083/createContent",
        {
            web_url : 'http://javacommunity.org/java/'+document.getElementById("file").value.toLowerCase()+'.html',
            desc : desc,
            package: document.getElementById("package").value.toLowerCase(),
            heading : heading,
            desc_heading : desc_heading
        },
        function(data, status){
            //alert("Data: " + data + "\nStatus: " + status);
            if(status == "success"){
                alert('File Updated Successfully');
            }

        });
}

// get description from db
function getDesc() {//alert(document.getElementById("editor").value);
    package = window.location.pathname.slice(window.location.pathname.indexOf('/')+1,window.location.pathname.lastIndexOf('/'));
    web_url = 'http://javacommunity.org/'+package+'/'+window.location.href.substring(window.location.href.lastIndexOf('/')+1,window.location.href.length);
    $.post("http://127.0.0.1:8083/fileDesc",
        {
            web_url : web_url,
            package : package
        },
        function(data, status){
            //alert("Data: " + data + "\nStatus: " + status);
            /* for (var j = 0; j < data.length; j++){
                 //console.log(data[j].web_name);
                 count = j+1;
                 var newDiv = $("<div id='desc"+count+"'><label id='rowNumber"+count+"'>"+count+"</label><textarea id='descText"+count+"' style='height:60px'>"+data[j].description+"</textarea></div>");
                 $('#description').append(newDiv);
             }*/
            document.getElementById("heading").innerHTML = data[0].heading;
            document.getElementById("desc_heading").innerHTML = data[0].desc_heading;
            console.log(data[0].heading);
            //description = description.replace(/\n\r?/g, '<br />');
            var descLines = data[0].description.split('_^');
            var results = [];
            if(descLines.length>1){
                $("#descList").empty();
                for( var i = 0; i < descLines.length; ++i ) {
                    //results.push( descLines[i] );
                    console.log(descLines[i]);
                    count = i+1;
                    descLines[i] = descLines[i].replace(/\t/g, '&nbsp;&nbsp;&nbsp;&nbsp;');
                    descLines[i] = descLines[i].replace(/\n\r?/g, '<br />');
                    //var newLine = $("<li class='li'>"+descLines[i].replace(/\n\r?/g, '<br />')+"</li>");
                    var newLine = $("<li class='li'>"+descLines[i]+"</li>");
                    //alert(descLines[i]);
                    $('#descList').append(newLine);
                }
            }else{
                $("#descList").empty();
                count = 1;
                var newLine = $("<li class='li'>"+data[0].description+"</li>");
                $('#descList').append(newLine);
            }
            var fileHeading = $("<h2 style='font-family:playfair;font-weight: bold;font-size:35px;' >"+data[0].heading+"</h2>");
            $('#column-web1').append(fileHeading);
        });
}
// get table names from db
// get code from db
function getcode(web_url) {
    // alert(window.location.href.substring(window.location.href.lastIndexOf('/')+1,window.location.href.length));
    package = window.location.pathname.slice(window.location.pathname.indexOf('/')+1,window.location.pathname.lastIndexOf('/'));
    //alert(window.location.pathname.slice(window.location.pathname.lastIndexOf('/')+1));
    if(!web_url ||  web_url == ''){
        web_url = 'http://javacommunity.org/'+package+'/'+window.location.href.substring(window.location.href.lastIndexOf('/')+1,window.location.href.length);
    }
    $.post("http://127.0.0.1:8083/code",
        {
            web_url : web_url,
            package : package
        },
        function(data, status){
            if(!data || data.trim() == ''){
                $('#code_div').class='hide';
            }
            $('#code_div').class='btn-div';
            // alert("Data: " + data + "\nStatus: " + status);
            document.getElementById("jsEditor").innerHTML = data;
            var e = ace.edit("jsEditor");
            //e.getSession().setMode("ace/mode/javascript");
            e.getSession().setMode("ace/mode/java");
            //e.getSession().setMode("ace/mode/c++");
            //e.getSession().setMode("ace/mode/html");
            e.setReadOnly(true);
            e.setOption("showPrintMargin", false)
            e.setTheme("ace/theme/github");
            e.getSession().setTabSize(4);
            e.setOptions({ maxLines: Infinity});
            e.getSession().setUseWrapMode(true);
            document.getElementById('theme').onchange = function() {
                var x = document.getElementById("theme");
                var themeValue = x.options[x.selectedIndex].value;
                if (themeValue == "github") {
                    e.setTheme("ace/theme/github");
                }
                if (themeValue == "cobalt") {
                    e.setTheme("ace/theme/cobalt");
                }
                if (themeValue == "terminal") {
                    e.setTheme("ace/theme/terminal");
                }
                if (themeValue == "xcode") {
                    e.setTheme("ace/theme/xcode");
                }
                if (themeValue == "tn"){
                    e.setTheme("ace/theme/tomorrow_night");
                }
            }
            $(document).ready(function(){
                var originalSize = $(".resizeable").css("font-size");
                // reset
                $("#resetMe").click(function(){
                    $(".resizeable").css("font-size", originalSize);
                });

                // Increase Font Size
                $("#increase").click(function(){
                    var currentSize = $(".resizeable").css("font-size");
                    var currentSize = parseFloat(currentSize)*1.2;
                    $(".resizeable").css("font-size", currentSize);
                    return false;
                });

                // Decrease Font Size
                $("#decrease").click(function(){
                    var currentFontSize = $(".resizeable").css("font-size");
                    var currentSize = $(".resizeable").css("font-size");
                    var currentSize = parseFloat(currentSize)*0.8;
                    $(".resizeable").css("font-size", currentSize);
                    return false;
                });
            });

            $('#copy').on('click', function() {
                var sel = e.selection.toJSON();
                e.selectAll();
                e.focus();
                document.execCommand('copy');
                e.selection.fromJSON(sel);
                document.getElementById("copy-message").innerHTML = "Code Copied";
                setTimeout(function () {document.getElementById('copy-message').innerHTML = '';}, 2000);
            });
        });
}


